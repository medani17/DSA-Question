// Input: s = ["h","e","l","l","o"]
// output: ["o","l","l","e","h"]

code: 
  void reverseString(vector<char>& s) {
        int st = 0;
        int e = s.size()-1;
         while(st<e){
             swap(s[st++], s[e--]);
         }
    }

OR 
  void reverseString(vector<char>& s) {
        int res = s;
       reverse(res.begin(),res.end());
  
    }
