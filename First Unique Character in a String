// Input: s = "leetcode"
// Output: 0

code:
 int firstUniqChar(string s){
  for(int i=0; i<s.length(); i++){
    bool flag = false;
   for(int j=0; j<length(); j++){
     if(i != j && s[i] == s[j]){
      flag = true;
      break;
      }
   }
  if(flag == false){
  return i;
  }
}
return -1;
}

 
